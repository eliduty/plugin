{
  "name": "vite-plugin-iconfont",
  "version": "1.0.8",
  "description": "This plugin is for auto download iconfont symbol js file",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliduty/vite-plugin-iconfont.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vite-plugin",
    "iconfont"
  ],
  "author": {
    "name": "eliduty",
    "email": "eliduty@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eliduty/vite-plugin-iconfont/issues"
  },
  "homepage": "https://github.com/eliduty/vite-plugin-iconfont#readme",

  "dependencies": {
    "x2js": "^3.4.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "release-it": "^15.6.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "rimraf": "^4.1.2"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}",
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": false
    },
    "hooks": {
      "before:init": "pnpm build"
    }
  }
}
