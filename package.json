{
  "name": "@eliduty/plugin",
  "version": "1.2.4",
  "description": "插件库",
  "homepage": "https://github.com/eliduty/plugin#readme",
  "bugs": {
    "url": "https://github.com/eliduty/plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliduty/plugin.git"
  },
  "license": "MIT",
  "author": {
    "name": "eliduty",
    "email": "eliduty@qq.com"
  },
  "contributors": [
    {
      "name": "eliduty",
      "email": "eliduty@qq.com",
      "url": "https://github.com/eliduty"
    }
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "simple-git-hooks",
    "build": "pnpm -r --filter=./packages/* run build",
    "taze": "taze minor -Ir",
    "typecheck": "tsc --noEmit",
    "release:pkg": "pnpm -r --filter=./packages/* run release",
    "release": "release-it",
    "dev:example": "pnpm --F example dev",
    "build:example": "pnpm --F example build-only",
    "dev": "vitest",
    "test": "vitest run",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@eliduty/eslint-config": "^1.1.1",
    "@eliduty/prettier-config": "^1.0.22",
    "@eliduty/tsconfig": "^1.1.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/node": "^20.11.13",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "jsdom": "^24.0.0",
    "msw": "^2.1.5",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.15.1",
    "prettier": "^3.2.4",
    "release-it": "^17.0.3",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite-plugin-iconfont": "workspace:^",
    "vitest": "^1.2.2"
  },
  "packageManager": "pnpm@8.15.1"
}
