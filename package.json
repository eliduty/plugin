{
  "name": "@eliduty/plugin",
  "version": "1.2.4",
  "packageManager": "pnpm@9.7.1",
  "description": "插件库",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/eliduty/plugin#readme",
  "bugs": {
    "url": "https://github.com/eliduty/plugin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliduty/plugin.git"
  },
  "author": {
    "name": "eliduty",
    "email": "eliduty@qq.com"
  },
  "contributors": [
    {
      "name": "eliduty",
      "email": "eliduty@qq.com",
      "url": "https://github.com/eliduty"
    }
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "postinstall": "simple-git-hooks",
    "build": "pnpm -r --filter=./packages/* run build",
    "taze": "taze minor -Ir",
    "typecheck": "tsc --noEmit",
    "release:pkg": "pnpm -r --filter=./packages/* run release",
    "release": "release-it",
    "dev:example": "pnpm --F example dev",
    "build:example": "pnpm --F example build-only",
    "dev": "vitest",
    "test": "vitest run",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@eliduty/eslint-config": "^2.1.0",
    "@eliduty/prettier-config": "^1.0.24",
    "@eliduty/tsconfig": "^1.2.3",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/node": "^22.3.0",
    "chalk": "^5.3.0",
    "eslint": "^9.9.0",
    "jsdom": "^24.1.1",
    "msw": "^2.3.5",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^9.7.1",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.6",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite-plugin-iconfont": "workspace:^",
    "vitest": "^2.0.5"
  }
}
